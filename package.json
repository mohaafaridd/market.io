{
	"name": "market.io",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node app.js",
		"start:prod": "concurrently \"npm start\" \"npm run client\"",
		"server": "nodemon app.js",
		"client": "npm start --prefix client",
		"clientinstall": "npm install --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
	},
	"engines": {
		"node": "10.3.0"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"bcryptjs": "^2.4.3",
		"cookie-parser": "~1.4.3",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^8.1.0",
		"env-cmd": "^10.1.0",
		"express": "~4.16.0",
		"express-jwt": "^5.3.1",
		"express-validator": "^6.2.0",
		"faker": "^4.1.0",
		"jest": "^24.9.0",
		"js-cookie": "^2.2.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"mongoose": "^5.6.10",
		"mongoose-unique-validator": "^2.0.3",
		"morgan": "~1.9.0",
		"ms": "^2.1.2",
		"multer": "^1.4.2",
		"numeral": "^2.0.6",
		"sharp": "^0.30.5",
		"supertest": "^4.0.2",
		"validator": "^11.1.0"
	},
	"devDependencies": {
		"concurrently": "^5.0.0",
		"nodemon": "^1.19.4",
		"prettier": "^1.19.1"
	}
}
